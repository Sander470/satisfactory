# EditorConfig is awesome: https://editorconfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4

[*.{yml,yaml}]
indent_size = 2

[*.{js,py}]
indent_size = 4

[lib/**.js]
indent_size = 2

[package.json]
indent_size = 2
